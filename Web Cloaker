javascript:document.getElementById('saveButton').addEventListener('click', function() {var selectedPreset = document.getElementById('presetSelect').value;document.getElementById('success').style.display = 'block';document.getElementById('success').textContent = 'Preset cloak selected: ' + selectedPreset;});document.getElementById('resetButton').addEventListener('click', function() {document.getElementById('success').style.display = 'none';document.getElementById('presetSelect').selectedIndex = 0;});
